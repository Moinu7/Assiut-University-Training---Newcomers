#include <iostream>

int main() {
    int n;
    std::cin >> n;

    int years = n / 365;
    n %= 365; 

    int months = n / 30;
    int days = n % 30; 

    std::cout << years << " years" << std::endl;
    std::cout << months << " months" << std::endl;
    std::cout << days << " days" << std::endl;

    return 0;
}
