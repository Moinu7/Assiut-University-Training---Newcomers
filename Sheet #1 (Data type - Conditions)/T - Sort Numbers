#include <iostream>
#include <vector>
#include <algorithm>

int main() {
    int a, b, c;
    std::cin >> a >> b >> c;

    std::vector<int> numbers = {a, b, c};
    std::vector<int> originalNumbers = {a, b, c}; 

    std::sort(numbers.begin(), numbers.end());

    for (int num : numbers) {
        std::cout << num << std::endl;
    }

    std::cout << std::endl; 

    for (int num : originalNumbers) {
        std::cout << num << std::endl;
    }

    return 0;
}
